<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter
        xmlns="https://github.com/spotbugs/filter/3.0.0"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="https://github.com/spotbugs/filter/3.0.0 https://raw.githubusercontent.com/spotbugs/spotbugs/3.1.0/spotbugs/etc/findbugsfilter.xsd">

    <Match>
        <Class name="uk.nhs.adaptors.pss.translator.model.DateFormat"/>
        <Bug pattern="EI_EXPOSE_REP2"/>
    </Match>

    <Match>
        <Class name="uk.nhs.adaptors.connector.service.MigrationStatusLogService"/>
        <Bug pattern="EI_EXPOSE_REP2"/>
    </Match>

    <Match>
        <Class name="uk.nhs.adaptors.common.util.fhir.FhirParser"/>
        <Bug pattern="EI_EXPOSE_REP2"/>
    </Match>

    <Match>
        <Class name="uk.nhs.adaptors.connector.service.PatientAttachmentLogService"/>
        <Bug pattern="EI_EXPOSE_REP2"/>
    </Match>

    <Match>
        <Class name="uk.nhs.adaptors.pss.translator.service.AttachmentHandlerService"/>
        <Bug pattern="DMI_INVOKING_TOSTRING_ON_ARRAY"/>
    </Match>

    <Match>
        <Class name="uk.nhs.adaptors.pss.translator.task.COPCMessageHandlerTest"/>
        <Bug pattern="UWF_UNWRITTEN_FIELD"/>
    </Match>
  
    <Match>
        <Class name="uk.nhs.adaptors.connector.service.PatientMigrationRequestService"/>
        <Bug pattern="EI_EXPOSE_REP2"/>
    </Match>

    <Match>
        <class name="uk.nhs.adaptors.pss.translator.util" />
        <Bug pattern="MS_EXPOSE_REP"/>
    </Match>

</FindBugsFilter>
